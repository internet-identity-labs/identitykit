{
  "name": "@nfid/docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:check": "next lint",
    "lint:fix": "next lint --fix",
    "format:write": "prettier --write '**/*.{ts,tsx,md,mdx,css}'",
    "lint-staged": "lint-staged --no-stash"
  },
  "dependencies": {
    "eslint-config-next": "^14.2.33",
    "next": "^14.2.33",
    "nextra": "^3.0.2",
    "nextra-theme-docs": "^3.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@radix-ui/react-icons": "^1.3.2",
    "@types/react": "^18.3.27",
    "lint-staged": "^15.2.11",
    "prettier": "^3.7.1"
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx}": [
      "npm run lint:fix"
    ],
    "**/*.{js,jsx,ts,tsx,md,json}": [
      "npm run format:write"
    ]
  }
}
