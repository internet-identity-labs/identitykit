{
  "name": "@nfid/identitykit",
  "version": "0.0.0-alpha.1",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./react": "./src/libs/react/index.ts",
    "./react/styles.css": "./src/libs/react/styles.css"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "rollup -c --bundleConfigAsCjs --watch",
    "build": "npm run rollup",
    "post:build": "node ./scripts/frankBuild.js",
    "rollup": "rollup -c --bundleConfigAsCjs",
    "test": "jest --passWithNoTests",
    "lint:check": "eslint . --ext ts,tsx --max-warnings 0",
    "lint:fix": "eslint . --fix --ext ts,tsx --max-warnings 0",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format:write": "prettier --write '**/*.{ts,tsx,md,mdx,css}'",
    "lint-staged": "lint-staged --no-stash"
  },
  "size-limit": [
    {
      "path": "dist/react-lib.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-lib.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@headlessui/react": "2.1.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/themes": "^3.0.5",
    "@slide-computer/signer-agent": "^2.6.2",
    "@slide-computer/signer": "^2.6.2",
    "@slide-computer/signer-storage": "^2.6.2",
    "@slide-computer/signer-web": "^2.6.2",
    "buffer": "^6.0.3",
    "clsx": "^2.1.1",
    "tiny-invariant": "^1.3.1",
    "react-hook-form": "^7.52.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-url": "^8.0.2",
    "@svgr/rollup": "^8.1.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.19.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-rename": "^1.0.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "size-limit": "^11.1.4",
    "storybook": "^8.1.5"
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx}": [
      "npm run lint:fix"
    ],
    "**/*.{js,jsx,ts,tsx,md,json}": [
      "npm run format:write"
    ]
  }
}
