{
  "name": "@nfid/identitykit",
  "version": "0.0.0-alpha.5",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "main": "./dist/index.cjs.js",
      "module": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "main": "./dist/libs/react/index.cjs.js",
      "module": "./dist/libs/react/index.esm.js",
      "types": "./dist/libs/react/index.d.ts"
    },
    "./react/styles.css": {
      "import": "./dist/libs/react/index.css",
      "require": "./dist/libs/react/index.css",
      "default": "./dist/libs/react/index.css"
    }
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "rollup -c --bundleConfigAsCjs --watch",
    "build": "rm -rf ./dist && npm run rollup",
    "post:build": "node ./scripts/frankBuild.js",
    "rollup": "rollup -c --bundleConfigAsCjs",
    "test": "jest --passWithNoTests",
    "lint:check": "eslint . --ext ts,tsx --max-warnings 0",
    "lint:fix": "eslint . --fix --ext ts,tsx --max-warnings 0",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format:write": "prettier --write '**/*.{ts,tsx,md,mdx,css}'",
    "lint-staged": "lint-staged --no-stash"
  },
  "size-limit": [
    {
      "path": "dist/libs/react/index.cjs.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/libs/react/index.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@headlessui/react": "2.1.3",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@slide-computer/signer": "^3.1.0",
    "@slide-computer/signer-agent": "^3.1.0",
    "@slide-computer/signer-storage": "^3.1.0",
    "@slide-computer/signer-transport-plug": "^3.1.0",
    "@slide-computer/signer-web": "^3.1.0",
    "clsx": "^2.1.1",
    "react-hook-form": "^7.53.0",
    "tailwindcss": "^3.4.10"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@svgr/rollup": "^8.1.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.0",
    "eslint-plugin-jest": "^28.8.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.21.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "size-limit": "^11.1.4",
    "storybook": "^8.2.9",
    "ts-jest": "^29.2.5"
  },
  "peerDependencies": {
    "react": ">=18.3.1",
    "react-dom": ">=18.3.1",
    "@dfinity/ledger-icp": ">=2.4.0"
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx}": [
      "npm run lint:fix"
    ],
    "**/*.{js,jsx,ts,tsx,md,json}": [
      "npm run format:write"
    ]
  }
}
